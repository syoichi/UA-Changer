{
    "name": "UA Changer",
    "version": "0.0.1",
    "manifest_version": 2,
    "description": "change User Agent.",
    "background": {
        "scripts": ["background.js"]
    },
    "permissions": [
        "webRequest",
        "webRequestBlocking",
        "*://www.tumblr.com/new/*"
    ],
    "minimum_chrome_version": "25"
}
